# Top-most EditorConfig file.
root = true

# For all files ...
[*]
# Unix-style newlines with a newline ending every file.
end_of_line = lf
# Insert final newline.
insert_final_newline = true
# Indents are tabs and have a size of 4 spaces.
indent_style = tab
indent_size = 4
# Use UTF-8.
charset = utf-8
